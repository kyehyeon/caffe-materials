name: "PVANET"

################################################################################
## Input
################################################################################

layer {
  name: 'input-data'
  type: 'Python'
  top: 'data'
  top: 'label'
  include { phase: TRAIN }
  python_param {
    module: 'od_data_layer'
    layer: 'ODDataLayer'
    param_str: '{ "source": "caffe-materials/practice6/voc2007.txt", "img_dir": "data/VOC2007/JPEGImages", "mean": [103, 116, 123], "base_size": [640] }'
  }
}


################################################################################
## Conv 1
################################################################################
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data"
  top: "conv1"
  param { lr_mult: 0.1  decay_mult: 0.1 }
  param { lr_mult: 0.1  decay_mult: 0.1 }
  convolution_param {
    num_output: 32
    kernel_size: 4  stride: 2  pad: 1
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" }
  }
}
layer {
  name: "conv1/bn"
  type: "BatchNorm"
  bottom: "conv1"
  top: "conv1"
  batch_norm_param { use_global_stats: true }
}
layer {
  name: "conv1/scale"
  type: "Scale"
  bottom: "conv1"
  top: "conv1"
  param { lr_mult: 0.1  decay_mult: 0.1 }
  param { lr_mult: 0.1  decay_mult: 0.1 }
  scale_param { bias_term: true }
}
layer {
  name: "relu1"
  type: "ReLU"
  bottom: "conv1"
  top: "conv1"
}

################################################################################
## Conv 2
################################################################################
layer {
  name: "conv2"
  type: "Convolution"
  bottom: "conv1"
  top: "conv2"
  param { lr_mult: 0.1  decay_mult: 0.1 }
  param { lr_mult: 0.1  decay_mult: 0.1 }
  convolution_param {
    num_output: 48
    kernel_size: 3  stride: 2  pad: 1
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" }
  }
}
layer {
  name: "conv2/bn"
  type: "BatchNorm"
  bottom: "conv2"
  top: "conv2"
  batch_norm_param { use_global_stats: true }
}
layer {
  name: "conv2/scale"
  type: "Scale"
  bottom: "conv2"
  top: "conv2"
  param { lr_mult: 0.1  decay_mult: 0.1 }
  param { lr_mult: 0.1  decay_mult: 0.1 }
  scale_param { bias_term: true }
}
layer {
  name: "relu2"
  type: "ReLU"
  bottom: "conv2"
  top: "conv2"
}

################################################################################
## Conv 3
################################################################################
layer {
  name: "conv3"
  type: "Convolution"
  bottom: "conv2"
  top: "conv3"
  param { lr_mult: 0.1  decay_mult: 0.1 }
  param { lr_mult: 0.1  decay_mult: 0.1 }
  convolution_param {
    num_output: 96
    kernel_size: 3  stride: 2  pad: 1
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" }
